{
  "Logging": {
    "LogLevel": {
      "Default": "Warning"
    }
  },
  "AllowedHosts": "*",
  "Serilog": {
    "WriteTo": [
      {
        "Name": "Loggly",
        "Args": {
          "customerToken": "xxxxxxxx-xxxx-xxxx-xxxx-xxxxxxxxxxxx",
          "tags": "dixit"
        }
      }
    ],
    "Properties": { "Application": "Dixit" }
  },
  "FaunaDb": {
    "Endpoint": "ttps://db.fauna.com:443",
    "Secret": "<<YOUR-SECRET-HERE>>",
    "ConnectionIndex": "connections",
    "SessionIndex":  "sessions"
  }
}
